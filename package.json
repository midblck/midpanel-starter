{
  "name": "midblck-admin-starter",
  "version": "1.1.0",
  "description": "A modern PayloadCMS admin starter template with Next.js, TypeScript, and shadcn/ui components",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "set NODE_OPTIONS=--no-deprecation && set NODE_OPTIONS=%NODE_OPTIONS% --max-old-space-size=8000 && pnpm generate:importmap && pnpm generate:types && next build --webpack",
    "dev": "set NODE_OPTIONS=--no-deprecation && next dev",
    "dev:clean": "rm -rf .next-dev && set NODE_OPTIONS=--no-deprecation && next dev",
    "generate:importmap": "set NODE_OPTIONS=--no-deprecation && payload generate:importmap",
    "generate:types": "set NODE_OPTIONS=--no-deprecation && payload generate:types",
    "lint": "set NODE_OPTIONS=--no-deprecation && eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "set NODE_OPTIONS=--no-deprecation && node scripts/lint-fix.js",
    "format": "set NODE_OPTIONS=--no-deprecation && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"*.{js,mjs,cjs,json,md}\"",
    "format:check": "set NODE_OPTIONS=--no-deprecation && prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"*.{js,mjs,cjs,json,md}\"",
    "format:fix": "set NODE_OPTIONS=--no-deprecation && pnpm format && pnpm lint:fix",
    "ci:check": "set NODE_OPTIONS=--no-deprecation && pnpm typecheck && pnpm lint && pnpm format:check",
    "typecheck": "tsc --noEmit",
    "payload": "set NODE_OPTIONS=--no-deprecation && payload",
    "start": "set NODE_OPTIONS=--no-deprecation && next start",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next .next-dev",
    "clean:dev": "rm -rf .next-dev",
    "clean:build": "rm -rf .next",
    "turbo:build": "turbo run build",
    "turbo:dev": "turbo run dev",
    "turbo:lint": "turbo run lint",
    "prepare": "husky install",
    "setup:dev": "node scripts/setup-dev.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-mongodb": "^3.72.0",
    "@payloadcms/next": "^3.72.0",
    "@payloadcms/payload-cloud": "^3.72.0",
    "@payloadcms/richtext-lexical": "^3.72.0",
    "@payloadcms/storage-uploadthing": "^3.72.0",
    "@payloadcms/translations": "^3.72.0",
    "@payloadcms/ui": "^3.72.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^11.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "16.4.7",
    "google-auth-library": "^10.4.1",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "kbar": "0.1.0-beta.48",
    "lucide-react": "^0.376.0",
    "next": "^16.1.3",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.0",
    "payload": "^3.72.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.65.0",
    "react-swipeable-button": "^1.6.5",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "styled-jsx": "^5.1.6",
    "swr": "^2.2.5",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.5.4",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.11",
    "eslint": "^9.16.0",
    "eslint-config-next": "^16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^2.7.5",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.50.0"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,mjs,cjs,json,md}": [
      "prettier --write"
    ]
  }
}
